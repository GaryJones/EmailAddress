{
    "name": "gamajo/email-address",
    "description": "Value Object representing an email address.",
    "keywords": [
        "email address"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Gary Jones",
            "homepage": "https://gamajo.com"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4",
        "object-calisthenics/phpcs-calisthenics-rules": "^2",
        "wimg/php-compatibility": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Gamajo\\EmailAddress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gamajo\\EmailAddress\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "install-codestandards": [
            "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
      ]
  }
}
